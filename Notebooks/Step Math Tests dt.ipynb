   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "t = [1,2,1,3,3,1,0,0,3,2,1]\n",
    "x = np.arange(len(t))\n",
    "\n",
    "def H(y,a,b):\n",
    "    return np.heaviside(y-a-1,1) - np.heaviside(y-b-1,1)\n",
    "\n",
    "plt.step(x,t)\n",
    "plt.step(x,2*H(x,3,5)-1*H(x,4,6)+2*H(x,5,8))"
   ]
